# --- Supabase (public) --- used by browser client, server components, and middleware
NEXT_PUBLIC_SITE_URL=
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# --- Supabase (private) --- used by admin client in cron job only; not needed for local dev unless testing cron
SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=

# --- Email --- used by cron job to send error notifications
EMAIL_HOST=smtp.gmail.com
EMAIL_USER=
EMAIL_PASS=
CRON_ERROR_EMAIL=

# --- AI Keys --- used by cron job to send error notifications
OPENAI_API_KEY=
OPENAI_MODEL=gpt-5.2-mini

# --- Cron Secret --- used by cron job to authenticate
CRON_SECRET=

# --- AI Concurrency --- used by cron job to limit the number of concurrent AI requests
AI_CONCURRENCY=4